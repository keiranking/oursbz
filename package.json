{
  "name": "oursbz",
  "version": "1.0.0",
  "main": "index.html",
  "scripts": {
    "build": "npm run build:js && npm run build:css && eleventy",
    "build:css": "sass src/assets/styles:_site/css",
    "build:js": "npx esbuild src/config/app.js src/config/carousel.js --bundle --outdir=_site/js --minify",
    "start": "npx concurrently \"npm run watch:js\" \"npm run watch:css\" \"npx @11ty/eleventy --serve\"",
    "watch:css": "npx sass src/assets/styles:_site/css --watch",
    "watch:js": "npx esbuild src/config/app.js src/config/carousel.js --bundle --outdir=_site/js --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-navigation": "^1.0.4",
    "dayjs": "^1.11.13",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "js-yaml": "^4.1.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "sass": "^1.89.2",
    "stimulus": "^3.2.2"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "esbuild": "^0.25.6"
  }
}
